<%- include("partials/header"); -%>

    <div class="">
      <h1>Compose</h1>
    </div>

    <form class="" action="/compose" method="post">

      <div class="form-group">
        <label class="form-label">Title</label>
        <input type="text" class="form-control" name="postTitle"autocomplete="off">

        <label class="form-label">Post</label>
        <textarea  class="form-control" name="postBody" rows="3" cols="30"></textarea>
      </div>

      <div class="form-group">
        <button type="submit" class="btn btn-primary" name="Publish" value="">Publish</button>
      </div>

    </form>

<%- include("partials/footer"); -%>
